{
  "name": "mui-formily",
  "version": "0.2.26",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "author": {
    "name": "liudichen",
    "email": "liudichen@foxmail.com",
    "url": "https://www.iimm.ink"
  },
  "description": "mui formily form field",
  "repository": {
    "url": "https://github.com/liudichen/mui-formily",
    "type": "https"
  },
  "keywords": [
    "mui",
    "material-ui",
    "formily",
    "form",
    "iimm",
    "www.iimm.ink",
    "liudichen",
    "react",
    "@mui/material",
    "layout",
    "react"
  ],
  "files": [
    "es",
    "lib"
  ],
  "license": "MIT",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@formily/core": "^2.1.11",
    "@formily/react": "^2.1.11",
    "@formily/reactive": "^2.1.11",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "5.0.0-alpha.74",
    "@mui/material": "^5.9.2",
    "ahooks": "^3.5.2",
    "mui-component": "^0.1.34",
    "mui-formfield": "^0.2.49",
    "prop-types": "^15.8.1",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.1.0",
    "eslint": "^7.32.0",
    "eslint-config-egg": "^11.0.1",
    "eslint-plugin-react": "^7.29.4",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "typescript": "^4.6.3",
    "yorkie": "^2.0.0"
  }
}
